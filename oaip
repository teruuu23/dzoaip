//задание 1
#include <iostream>
используя пространство имен std;
int main() {
    интервал см; // длина в сантиметрах
    cout << "Введите длину в сантиметрах: ";
    цин >> см;
    инт метров = см/100; //метров
    int cmt = см % 100; // действие сантиметры
    cout << "метры" << метры << " ";
    cout << " сантиметры " << cmt << " ";
    вернуть 0;
}  



//задание 2
#include <iostream>
используя пространство имен std;
int main() {
    интервал а, б; //длина и ширина
    cout << "длина";
    цин >> а;
    cout << "ширина";
    цин >> б;
    int s = a*b;
    cout << "площадь" << s;
    вернуть 0;
}


// задача 3
#include <iostream>
используя пространство имен std;
