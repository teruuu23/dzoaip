    cout << "темп фаренг" << fa << endl;
    вернуть 0;
}


//задание 4
#include <iostream>
используя пространство имен std;
int main() {
    int ch;
    cout << "число";
    cin >> ch;
    если (ч % 2 == 0) {
        cout << "четное";
    } еще {
    cout << "нечетное";
    }
    вернуть 0;
}

//задание 5
#include <iostream>
используя пространство имен std;
int main() {
    int ch;
    cout << "ввести число";
    cin >> ch;
    если (ch % 5 == 0 && ch % 3 == 0) {
        cout << "число молодец"; 
    }
    иначе если (ch % 5 == 0) {
        cout << "только 5";
    }
    иначе если (ch % 3 == 0) {
        cout << "только 5";
    }
    вернуть 0;
}

//задание 6
#include <iostream>
используя пространство имен std;
int main() {
    int ch;
    cout << "ввести число";
    cin >> ch;
    если (ч < 18) {
        cout << "несовершеннолетний гад";
    }
    иначе если (ch >= 18 && ch <= 60) {
        cout << "вы молодец";
    }
    иначе если (ch > 60) {
        cout << "пора отдыхать";
    }
    вернуть 0;
}

//задание 7
#include <iostream>
используя пространство имен std;
int main() {
    int n;
    cout << "вести n";
    цин >> н;
    для (int i = 1; i <= n; i++) {
        cout << i << " ^2 =" << i * i << endl;
    }
    вернуть 0;
}

// Задание 8
#include <iostream>
#include <строка>
используя пространство имен std;
int main() {
	int флаг = 1;
	int k = 0;
	строка Ls[] = {"e","y","u","i","o","a","E","Y","U","I","O","A"};
	в то время как (флаг != 0) {
		строка а;
		cout << "Введите букву:"<< endl;
		цин >> а;
	  если (а == "0"){
			флаг = 0;
			перерыв;
		}

		для (int i = 0; i < 12; i++) {
			если (Ls[i] == a) {
				к += 1;
			}
		}
	}
	cout << "Кол-во гл:" << k << endl;
	вернуть 0;
}

//Задание 9
#include <iostream>
используя пространство имен std;
int main() {
	int n;
	cout << "Введите количество элементов:";
  цин >> н;
	int S = 0;
	для (int i = 1; i < (n+1); i++) {
		если ((i % 2 == 0) && (i % 5 == 0)) {
			S += i;
		}
		иначе если (i % 2 == 0){
			S += i;
		}
		иначе если (i % 5 == 0){
			S += i;
		}
	
	cout << "Сумма всех элементов:" << S;
	вернуть 0;
}

// Задание 10
#include <iostream>
используя пространство имен std;
int main(){
	int а;
	cout << "Ввести число:";
	цин >> а;
	если (а == 0) {
		cout << "ноль";
	}
	иначе если (a > 0){
	  cout << "полож";
	}
	иначе если (a < 0){
		cout << "отриц";
	}
	вернуть 0;
}

Задание 11
#include <iostream>
используя пространство имен std;
int main() {
	int k = 32;
	для (char i = ' '; i <= '~';i++) {
		cout << k << "-й элемент: "<< i << endl;
		к++;
	}
	вернуть 0;
}
